<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.hutu.cloud</groupId>
        <artifactId>hutu-starters</artifactId>
        <version>${revision}</version>
    </parent>

    <artifactId>hutu-cloud-starter</artifactId>
    <packaging>jar</packaging>
    <description>聚合项目</description>

    <dependencies>
        <!--配置中心-->
        <dependency>
            <groupId>com.hutu.cloud</groupId>
            <artifactId>hutu-cloud-starter-config</artifactId>
            <version>${revision}</version>
        </dependency>
        <!--注册中心依赖-->
        <dependency>
            <groupId>com.hutu.cloud</groupId>
            <artifactId>hutu-cloud-starter-register-center</artifactId>
            <version>${revision}</version>
        </dependency>
        <!--spring web 服务依赖-->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>
        <!--hibernate-validator-->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-validation</artifactId>
        </dependency>
        <!--Feign-->
        <dependency>
            <groupId>com.hutu.cloud</groupId>
            <artifactId>hutu-cloud-starter-feign</artifactId>
            <version>${revision}</version>
        </dependency>
        <!--安全模块-->
        <dependency>
            <groupId>com.hutu.cloud</groupId>
            <artifactId>hutu-cloud-starter-security</artifactId>
            <version>${revision}</version>
        </dependency>
        <!--处理敏感数据模块-->
        <dependency>
            <groupId>com.hutu.cloud</groupId>
            <artifactId>hutu-cloud-starter-jackson</artifactId>
            <version>${revision}</version>
        </dependency>
        <!--监控-->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-actuator</artifactId>
        </dependency>
        <!--bootstrap 启动器 新版版(2020.x.x)中默认关闭-->
        <!--<dependency>
            <groupId>org.springframework.cloud</groupId>
            <artifactId>spring-cloud-starter-bootstrap</artifactId>
        </dependency>-->
    </dependencies>

</project>
