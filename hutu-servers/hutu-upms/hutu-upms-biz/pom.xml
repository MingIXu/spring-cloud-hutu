            <artifactId>hutu-cloud-starter</artifactId>
            <version>${revision}</version>
        </dependency>

        <dependency>
            <groupId>com.hutu.cloud</groupId>
            <artifactId>hutu-cloud-starter-mybatis</artifactId>
            <version>${revision}</version>
        </dependency>
        <dependency>
            <groupId>com.hutu.cloud</groupId>
            <artifactId>hutu-cloud-starter-redisson</artifactId>
            <version>${revision}</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

</project>
